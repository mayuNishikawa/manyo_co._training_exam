<h2>管理画面のユーザー一覧画面</h2>
<%= link_to t('view.create_user'), new_admin_user_path %>
<% @users.each do |user| %>
  <%= user.id %>:<%= user.name %>
  <%= link_to t('view.edit'), edit_admin_user_path(user.id), class: "btn btn-outline-warning" %>
  <%= link_to t('view.destroy'), user, method: :delete, data: { confirm: t('view.are_you_sure?') }, class: "btn btn-outline-danger" %>
<% end %>